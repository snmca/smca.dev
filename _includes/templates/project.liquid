<article>
  {% assign slug = title | slug %}
  <h1 id="{{ slug }}" class="no-margin">
    <a class="header-anchor" href="#{{ slug }}" aria-hidden="true" tabindex="-1"></a>
    {{ title }}
  </h1>
  {{ content }}
</article>
